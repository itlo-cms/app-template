{
  "name": "paulzi/yii2-auto-tree",
  "description": "Allow apply multiple tree behavior for ActiveRecord in Yii2",
  "keywords": ["yii2", "auto tree", "adjacency list", "nested sets", "nested intervals", "materialized path"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/paulzi/yii2-auto-tree/issues?state=open",
    "source": "https://github.com/paulzi/yii2-auto-tree"
  },
  "authors": [
    {
      "name": "PaulZi",
      "email": "pavel.zimakoff@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "require-dev": {
    "paulzi/yii2-adjacency-list": "^2.0.3",
    "paulzi/yii2-nested-sets": "^1.0.4",
    "paulzi/yii2-nested-intervals": "^1.0.5",
    "paulzi/yii2-materialized-path": "^2.0.6",
    "phpunit/phpunit": "~4.0",
    "phpunit/dbunit": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "paulzi\\autotree\\": ""
    }
  }
}